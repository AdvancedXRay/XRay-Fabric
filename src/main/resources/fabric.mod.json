{
  "schemaVersion": 1,
  "id": "advanced-xray-fabric",
  "version": "${version}",
  "name": "Advanced XRay (Fabric)",
  "description": "An Advanced XRay mod for Fabric with all your block finding needs baked right in!",
  "authors": [
    "ErrorMikey"
  ],
  "contact": {
    "homepage": "https://github.com/AdvancedXRay/xray-fabric",
    "sources": "https://github.com/AdvancedXRay/xray-fabric"
  },
  "license": "MIT",
  "icon": "assets/advanced-xray-fabric/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "pro.mikey.fabric.xray.XRay"
    ]
  },
  "mixins": [
    {
      "config": "advanced-xray-fabric.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "fabric-api": "*",
    "minecraft": ">=1.20.6"
  },
  "accessWidener": "advanced-xray-fabric.accesswidener"
}
